public class Main {
    public static void main(String[] args) {
        System.out.println("Hello World!");
        int[] arr = {2,4,6,8,10};
        boolean result = false;
        for(int i=1;i<arr.length;i++){
            if(arr[i-1]<arr[i]){
                result=false;
                break;
            }
        }
        System.out.println(result);
    }
}
